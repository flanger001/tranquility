<nav class="header-container container">
  <%= render 'layouts/profile' %>

  <div class="site-header">
    <h1 class="site-header__title">
      A Touch Of Tranquility
    </h1>
    <div class="navigation">
      <% Navigation.site_links.each do |link| %>
        <%= link_to link.name, link.url, class: 'navigation__link-site' %>
      <% end %>
    </div>
    <div class="navigation">
      <% Navigation.product_links.each do |link| %>
        <%= link_to link.name, link.url, class: 'navigation__link-product' %>
      <% end %>
    </div>
  </div>
</nav>
